@article{Jiang_FMSDOB_J2018,
	Abstract = {Modulating the transmission of vibration energy in a closed-loop control system without sacrificing overall system performance is a fundamental issue in a wide range of applica- tions from precision motion control, active noise cancellation, to advanced manufacturing. As a powerful design method, local loop shaping has been recognized to be capable of actively and flexibly shaping system dynamics to respond to different shapes of input spectra. However, strong and flexible local loop shaping for systems with nonminimum-phase zeros has remained underdeveloped due to challenges to invert the system dynamics. Pursuing to address the problem, this paper proposes a new feedback approach to shape energy transmission with prescribed performance requirements. Pioneering an integration of the interpolation theory with a model-based parameterization of the closed loop, the work proposes a filter design that matches the inverse plant dynamics locally and creates a highly effective framework for controlling both narrow- and wide- band vibrations. Also discussed are optimization methods for controlling the fundamental waterbed limitation. The proposed algorithm is verified by simulation and experimentation on a laser scanning platform in selective laser sintering additive manufacturing. The benefit over conventional plant inverse-based loop-shaping method is also validated.},
	Author = {Tianyu Jiang and Hui Xiao and Jiong Tang and Liting Sun and Xu Chen},
	Date-Added = {2018-11-01 10:27:28 -0400},
	Date-Modified = {2018-12-05 22:29:13 -0500},
	Journal = {{IEEE} Transactions on Control Systems Technology},
	Keyword = {energy guiding, loop shaping},
	Note = {under review},
	Title = {Local Loop Shaping for Modulation of Narrow- and Wide-band Vibration Energy Transmission in Systems with Nonminimum-phase Zeros},
	Year = 2018,
	Bdsk-File-1 = {},
	Bdsk-File-2 = {}}

